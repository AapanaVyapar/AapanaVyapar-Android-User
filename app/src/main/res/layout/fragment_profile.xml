                android:scrollbarSize="8dp"
                android:text="AapanaVyapar"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="@+id/sign_in_logo2"
                app:layout_constraintStart_toStartOf="@+id/sign_in_logo2"
